<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bucketlist.controller.BucketlistListItem" table="bucketlistlistitem">
        <id name="itemId" type="java.lang.Integer">
            <column name="item_id" />
            <generator class="identity" />
        </id>
        <property name="content" type="string">
            <column name="item_content" length="200" />
        </property>
        <property name="description" type="string">
            <column name="item_description" length="1024" />
        </property>
        <property name="progress" type="java.lang.Integer">
            <column name="item_progress" />
        </property>
    </class>
</hibernate-mapping>
